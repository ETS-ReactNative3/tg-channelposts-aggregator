{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "entry"
      }
    ],
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      [
        {
          "rootPathSuffix": "src",
          "rootPathPrefix": "~"
        },
        {
          "rootPathSuffix": "node_modules",
          "rootPathPrefix": "Î·"
        }
      ],
      "react-hot-loader/babel"
    ]
  ]
}
